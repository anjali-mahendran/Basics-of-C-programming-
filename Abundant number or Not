#include<stdio.h>
int main()
{
    int n,sum,i;
    scanf("%d",&n);
    for(i=1;i<n;i++)
    {
     if(n%i==0)
      {
         sum=sum+i;
      }
    }
    if(sum>n)
      printf("the given number %d is an abundant number",n);
    else
      printf("the given number %d is not an abundant number",n);
}
