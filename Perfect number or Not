#include<stdio.h>
int main()
{
    int n,temp,i,sum=0;
    scanf("%d",&n);
    temp=n;
    for(i=1;i<n;i++)
    {
        if(n%i==0)
        {
            sum=sum+i;
        }
    }
    if(sum==temp)
      printf("the given number %d is perfect number",n);
    else
      printf("the given number %d is not a perfect number",n);
}
